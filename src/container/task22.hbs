<div>
  {{#with page_header}}
   <div class="text-center"><h1>{{title}}</h1></div>  
   <p><div class="text-uppercase text-center">{{description}}</div></p>
  {{/with}} 
  <br>
  <div class="card">
   <div class="card-body">
    {{#with section}}
     {{#with about_us}}
       {{#with title_list}}
         <div class="display-6 text-end">{{main}}</div> 
         <br>   
         <div class="text-end"><h2>{{sub}}</h2></div>
       {{/with}}
       <br>
       {{#with button}}
        <button class="btn btn-info ">{{text}}</button>
       {{/with}}
     {{/with}}
   </div>
  </div>
  <br>
  <div class="card">
    <div class="card-body">
     {{#with advantage}}
       <div class="display-5">{{title}}</div>
       <br>
       <div class="card">
       <div class="card-body">
        {{#with efficiency}}
        <div class="display-4">{{title}}</div>
        <br>
        <h6>{{sub_title}}</h6>
        <br>
        <p>{{description}}</p>
        <br>
        <button class="btn btn-outline-warning">{{button}}</button>
        {{/with}}
       </div>
       </div>
       <br>
       <div class="card">
       <div class="card-body">
        {{#with innovation}}
          <div class="display-4 text-center">{{title}}</div>
          <br>
          <div class="display-4 text-center"><h6>{{sub_title}}</h6></div>
          <br>
          <div class="display-4 text-center"><p>{{description}}</p></div>
          <br>
          <button class="btn btn-outline-warning">{{button}}</button>
        {{/with}}
       </div>
       </div>
       <br>
       <div class="card">
       <div class="card-body">
        {{#with collaboration}}
          <div class="display-4 text-end">{{title}}</div>
          <br>
          <div class="display-4 text-end"><h6>{{sub_title}}</h6></div>
          <br>
          <div class="display-4 text-end"><p>{{description}}</p></div>
          <br>
          <button class="btn btn-outline-warning">{{button}}</button>
        {{/with}}
       </div>
       </div>
     {{/with}}
    </div>
  </div>
  <br>
  <div class="card">
  <div class="card-body">
    {{#with agreement}}
      <div class="display-5">{{title}}</div>
      <br>
      <div class="card">
      <div class="card-body">
        {{#with info}}
       <h5>{{sub_title}}</h5>
       <br>
         <div class="card">
         <div class="card-body">
           {{#with detail}}
            <p>{{text}}</p>
            <button class="btn btn-success">{{button}}</button>
            {{/with}}
      </div>
      </div>
      {{/with}}
   {{/with}}
  </div>
  </div>


        
  {{/with}}

</div>